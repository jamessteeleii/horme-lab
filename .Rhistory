filter(row_number()==1)
lm1RM <- lm(one_RM ~ theta,
data = scores_oneRM)
beta_load <- comb_effort %>%
group_by(item) %>%
filter(row_number()==1) %>%
ggplot(aes(x = beta, y = as.numeric(item)*10)) +
